		<!-- Start image-gallery Area -->
			<section class="image-gallery-area section-gap">
				<div class="container">
					<div class="row section-title">
						<h1>Image Gallery that we like to share</h1>
						<p>Who are in extremely love with eco friendly system.</p>
					</div>					
					<div class="row">
					{{#if medias}}
						{{#each medias}}
						<div class="col-lg-4 single-gallery">
							<a href="/uploads/{{file}}" class="img-gal"><img class="img-fluid" src="/uploads/{{file}}" alt=""></a>
						</div>
						{{/each}}
					{{else}}
					<div class="alert alert-success text-center">No Gallery Photos to display</div>
					{{/if}}				
					</div>
				</div>	
			</section>
			<!-- End image-gallery Area -->